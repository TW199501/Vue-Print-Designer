{
  "name": "vue-print-designer",
  "private": false,
  "version": "0.9.2",
  "type": "module",
  "main": "dist/print-designer.umd.js",
  "module": "dist/print-designer.es.js",
  "types": "dist/web-component.d.ts",
  "style": "dist/print-designer.css",
  "exports": {
    ".": {
      "types": "./dist/web-component.d.ts",
      "import": "./dist/print-designer.es.js",
      "require": "./dist/print-designer.umd.js"
    },
    "./style.css": "./dist/print-designer.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:wc": "vue-tsc -b && vite build --config vite.config.wc.ts && node ./scripts/copy-wc-types.mjs",
    "preview": "vite preview"
  },
  "dependencies": {
    "@guolao/vue-monaco-editor": "^1.6.0",
    "canvg": "^4.0.3",
    "dom-to-image-more": "^3.7.2",
    "jszip": "^3.10.1",
    "jsbarcode": "^3.12.3",
    "jspdf": "^2.5.2",
    "lodash": "^4.17.21",
    "pinia": "^2.3.0",
    "qrcode": "^1.5.4",
    "uuid": "^11.0.3",
    "vue": "^3.5.13",
    "vue-i18n": "^11.2.8"
  },
  "devDependencies": {
    "@iconify-json/material-symbols": "^1.2.53",
    "@types/jsbarcode": "^3.11.4",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.2",
    "@types/qrcode": "^1.5.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "unplugin-icons": "^23.0.1",
    "vite": "^6.0.1",
    "vue-tsc": "^2.1.10"
  }
}
